      grid%p3d=domain_array
      call mpi_barrier(local_com, mpi_ierr)

      ! u3d
      do j=jps, jpe
       do k=1,num_metgrid_levels
        do i=ips, ipe
          patch_array(i,k,j)=grid%u3d(i,k,j)
