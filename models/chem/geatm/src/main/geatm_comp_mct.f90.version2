  subroutine geatm_export_mct( x2ge_ge, camgrid, loop )
!-----------------------------------------------------------------------
! Arguments
!
     use geatm_vartype, only:csx,cex,csy,cey    
     TYPE(camgrid_c) :: camgrid
     type(mct_aVect)   , intent(inout) :: x2ge_ge
     integer :: loop
 end subroutine geatm_export_mct
